#quick-slot-window-container
{
  position: absolute;
  left: 30%;
  bottom: 5%;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  border-color: #6b6b6b;
  border-width: 5px;
  padding: 0;
  margin: 0;
  flex-shrink: 1;
}

.quick-slot-window-item-box
{
  border-color: grey; 
  border-width: 1px;
  min-width: 150px;
  min-height: 130px;
}
